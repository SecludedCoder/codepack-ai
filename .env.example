# 环境变量配置示例
# 复制此文件为 .env.local 并填入实际值

# 应用配置
VITE_APP_TITLE=CodePack AI
VITE_APP_DESCRIPTION=智能代码打包工具

# API 配置（如果需要）
VITE_API_BASE_URL=https://api.example.com
VITE_API_KEY=your_api_key_here

# 功能开关
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=false

# 分析工具（可选）
VITE_GA_TRACKING_ID=
VITE_SENTRY_DSN=

# 构建配置
VITE_BUILD_TIMESTAMP=${new Date().toISOString()}
VITE_BUILD_VERSION=1.0.0

# 开发配置
VITE_DEV_SERVER_PORT=3000
VITE_DEV_HTTPS=false

# 部署配置
VITE_DEPLOY_URL=https://codepack.ai
VITE_CDN_URL=

# 特性标志
VITE_FEATURE_DARK_MODE=false
VITE_FEATURE_EXPORT_IMPORT=true
VITE_FEATURE_CLOUD_SYNC=false

# 限制配置
VITE_MAX_FILE_SIZE_MB=10
VITE_MAX_TOTAL_SIZE_MB=50
VITE_MAX_FILE_COUNT=1000

# 调试
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info